php artisan serve

php artisan make:controller HomeController
php artisan make:controller ArticleController --resource
php artisan make:controller Api/ArticleController --api


php artisan route:list
php artisan route:cache
php artisan route:clear

php artisan cache:clear
php artisan view:clear
php artisan config:clear

php artisan optimize

composer update

php artisan make:component InputText 
php artisan make:component InputText --view

php artisan migrate

php artisan make:migration create_articles_table  // create_ => oluştur, articles => dosya adı & tablo adı, _table => türü, --create=.... => tablo adı
php artisan make:migration create_articles_table  --create=sercan  // create_ => oluştur, articles => dosya adı, _table => türü, --create=.... => tablo adı